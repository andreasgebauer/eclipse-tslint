{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../bin",
    "sourceMap": true,
    "target": "es5",
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "types": [ ],
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "allowUnreachableCode": false,
    "pretty": true
  },
  "exclude": [
    "test.ts",
    "test/**",
    "**/*.spec.*",
    "node.d.ts"
  ],
  "compileOnSave": true
}
