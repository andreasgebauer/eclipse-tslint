{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "mapRoot": "./",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "../../bin",
    "sourceMap": true,
    "target": "es6",
    "typeRoots": [
      "../../node_modules/@types"
    ],
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "allowUnreachableCode": false,
    "pretty": true
  },
  "exclude": [ ],
  "compileOnSave": true
}
